
@using Dealership.Models ;
<h1>Cars List</h1>
<ul>

@if (Model.Count == 0)
{
  <p>You have no cars in your list!</p>
}
@if(@Model.Count != 0)
{
  @foreach (Car car in Model)
{
  <li>@car.GetMakeModel()  for @car.GetPrice() with @car.GetMiles()</li>
}
}


</ul>
<a href="/cars/new">Add a new car.</a>
<a href='/cars/checkprice'>Find car you need</a>
